    "babel-jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^2.79.2",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-terser": "^7.0.2",
